<%- include('partials/header'); -%>

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label for="exampleInputTitle1">Title</label>
    <input type="text" class="form-control" name="titlePost" aria-describedby="emailHelp">
  </div>
  <div class="form-group">
    <label for="exampleInputBody1">Post</label>
    <textarea class="form-control" name="bodyPost" id="floatingTextarea"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include('partials/footer'); -%>
